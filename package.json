{
  "name": "sindicato-ufm",
  "version": "4.0.0",
  "description": "",
  "main": "out/main.js",
  "scripts": {
    "build": "clj -m cljs.main --target node --output-to out/main.js -c sindicato-ufm.core",
    "start": "node ./out/main.js",
    "start:live": "nodemon"
  },
  "nodemonConfig": {
    "ignore": [
      ".cljs_node_repl",
      ".git",
      "node_modules"
    ],
    "watch": [
      "src"
    ],
    "exec": "npm run build && npm start",
    "ext": "cljs"
  },
  "author": "",
  "license": "MIT",
  "dependencies": {
    "express": "^4.17.1",
    "express-handlebars": "^5.2.1",
    "morgan": "^1.10.0",
    "source-map-support": "^0.5.19"
  },
  "devDependencies": {
    "nodemon": "^2.0.7"
  }
}
